{
    "name": "webhd/hd",
    "description": "WEBHD Wordpress Theme",
    "authors": [
        {
            "name": "NTH",
            "email": "info@webhd.vn"
        }
    ],
    "autoload": {
        "files": [
            "inc/ajax.php",
            "inc/post-types.php",
            "inc/helpers.php",
            "inc/image-sizes.php",
            "inc/menus.php",
            "inc/plugins.php",
            "inc/sidebars.php",
            "inc/template-functions.php",
            "inc/template-hooks.php",
            "inc/widgets.php"
        ],
        "psr-4": {
            "Webhd\\Blocks\\": "inc/Blocks",
            "Webhd\\Ext\\": "inc/Ext",
            "Webhd\\Helpers\\": "inc/Helpers",
            "Webhd\\Plugins\\": "inc/Plugins",
            "Webhd\\PostTypes\\": "inc/PostTypes",
            "Webhd\\Themes\\": "inc/Themes",
            "Webhd\\Widgets\\": "inc/Widgets"
        },
        "exclude-from-classmap": [
            "**/**/Elementor/Widgets/**"
        ]
    },
    "require": {
        "symfony/var-dumper": "^v5.3.8"
    }
